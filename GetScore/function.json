{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
     "name": "myPoints",
     "type": "table",
     "tableName": "teampoints",
     "filter": "teamId eq '{teamId}'",
     "connection": "AzureWebJobsStorage",
     "direction": "in"
    }
  ]
}
